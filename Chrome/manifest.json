{
	"name": "deviantAnywhere",
	"version": "1.0",
	"description": "Keeps deviantArt allways near you.",
	"browser_action": {
		"default_icon": "img/fella_icon.png"
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"css": ["css/intruder.css"],
		"js": ["js/jquery.js", "js/intruder.js"]
	}],
	
	"permissions":[
		"tabs", "*://*/*"
	]
}